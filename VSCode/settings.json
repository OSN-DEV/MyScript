          "meta.export.js",
          "source.js"
        ],
        "settings": {
          "foreground": "#C586C0",
        }
      },
      {
        "scope": [
          "storage.type.js",
          "entity.name.tag"
        ],
        "settings": {
          "foreground": "#569CD6",
        }
      },
      {
        "scope": [
          "variable",
          "support.type.property-name",
          "support.type.property-name.css",
          "entity.other.attribute-name"
        ],
        "settings": {
          "foreground": "#9CDCFE",
          "fontStyle": "",
        }
      },
      {
        "scope": "constant.numeric.decimal.js",
        "settings": {
          "foreground": "#B5CEA8",
        }
      },
      {
        "scope": [
